{
    "name": "astrotomic/iso639",
    "description": "ISO-639 PHP Library",
    "keywords": [
        "ISO 639",
        "ISO-639",
        "ISO",
        "639",
        "languages"
    ],
    "homepage": "https://github.com/Astrotomic/iso639",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://astrotomic.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\ISO639\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\ISO639\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}